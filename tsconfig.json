{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es6", "dom", "es2016"],
    "types": ["cypress"],
    "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "cypress/support/login/loginScreen"],
  "exclude": ["node_modules", "cypress.config.ts"]
}
